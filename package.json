{
  "name": "react-native-sortables",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@react-native/babel-preset": "^0.74.85",
    "@react-native/metro-config": "^0.74.85",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/react-native": "^12.5.1",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-react-native-matipl01": "2.1.0",
    "get-yarn-workspaces": "^1.0.2",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-native": "^0.74.3",
    "react-native-builder-bob": "^0.23.2",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "3.10.1",
    "react-test-renderer": "^18.3.1",
    "semantic-release": "^24.0.0",
    "syncpack": "^12.3.3",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format:code",
      "yarn lint:fix",
      "bash -c tsc --noEmit"
    ],
    "package.json": [
      "yarn format:deps"
    ]
  },
  "main": "dist/commonjs/index",
  "module": "dist/module/index",
  "packageManager": "yarn@4.1.1",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": "src/index",
  "scripts": {
    "build": "yarn clean && bob build",
    "clean": "./scripts/clean.sh",
    "example:app": "yarn workspace example-app",
    "example:bare": "yarn workspace example-bare",
    "example:expo": "yarn workspace example-expo",
    "format": "yarn format:code && yarn format:deps",
    "format:check": "prettier --check . --ignore-unknown",
    "format:code": "prettier --write . --ignore-unknown",
    "format:deps": "syncpack format",
    "husky:install": "./scripts/husky.sh",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "yarn husky:install",
    "prepare": "husky",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit && yarn example:app typecheck"
  },
  "type": "module",
  "types": "dist/typescript/index.d.ts",
  "workspaces": [
    "example/*",
    "src"
  ]
}
